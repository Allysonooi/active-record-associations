<p id="notice"><%= notice %></p>

<h1>Listing Adcoms</h1>

<table>
  <thead>
    <tr>
      <th>Admin</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @adcoms.each do |adcom| %>
      <tr>
        <td><%= adcom.admin %></td>
        <td><%= adcom.company %></td>
        <td><%= link_to 'Show', adcom %></td>
        <td><%= link_to 'Edit', edit_adcom_path(adcom) %></td>
        <td><%= link_to 'Destroy', adcom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Adcom', new_adcom_path %>
